{
  "id": null,
  "uid": null,
  "title": "Cache Monitoring",
  "tags": [],
  "timezone": "browser",
  "schemaVersion": 41,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "graph",
      "title": "Cache Size",
      "targets": [
        {
          "expr": "cache_size{cache=~\".*\"}",
          "legendFormat": "{{cache}}",
          "refId": "A"
        }
      ],
      "id": 1,
      "gridPos": { "h": 9, "w": 8, "x": 0, "y": 0 },
      "lines": true,
      "points": false,
      "linewidth": 2
    },
    {
      "type": "graph",
      "title": "Cache Hits",
      "targets": [
        {
          "expr": "cache_requests_hit{cache=~\".*\"}",
          "legendFormat": "{{cache}}",
          "refId": "B"
        }
      ],
      "id": 2,
      "gridPos": { "h": 9, "w": 8, "x": 8, "y": 0 },
      "lines": true,
      "points": false,
      "linewidth": 2
    },
    {
      "type": "graph",
      "title": "Cache Misses",
      "targets": [
        {
          "expr": "cache_requests_miss{cache=~\".*\"}",
          "legendFormat": "{{cache}}",
          "refId": "C"
        }
      ],
      "id": 3,
      "gridPos": { "h": 9, "w": 8, "x": 16, "y": 0 },
      "lines": true,
      "points": false,
      "linewidth": 2
    },
    {
      "type": "graph",
      "title": "Hit Ratio (%)",
      "targets": [
        {
          "expr": "cache_requests_hit{cache=~\".*\"} / (cache_requests_hit{cache=~\".*\"} + cache_requests_miss{cache=~\".*\"}) * 100",
          "legendFormat": "{{cache}}",
          "refId": "D"
        }
      ],
      "id": 4,
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 9 },
      "lines": true,
      "points": false,
      "linewidth": 2
    }
  ]
}
